{
    "name": "melsior/brain-games",
    "bin": [
        "bin/brain-balanced",
        "bin/brain-games",
        "bin/brain-even",
        "bin/brain-calc",
        "bin/brain-gcd"
    ],
    "description": "A training project in the hexlet.io team that develop small brain games for the console",
    "authors": [{
        "name": "Alexander Vasilyev",
        "email": "melsior@gmail.com"
    }],
    "minimum-stability": "dev",
    "version": "0.1.0",
    "scripts": {
        "phpcs": "phpcs"
    },
    "autoload": {
        "psr-4": {
            "BrainGames\\": "src/games/",
            "BrainEven\\": "src/games/",
            "BrainCalc\\": "src/games/",
            "BrainGcd\\": "src/games/",
            "BrainBalance\\": "src/games/",
            "GameLib\\": "src/",
            "CliMessage\\": "src/"
        },
        "files": [
            "src/games/BrainGames.php",
            "src/games/BrainEven.php",
            "src/games/BrainCalc.php",
            "src/games/BrainGcd.php",
            "src/games/BrainBalance.php",
            "src/GameLib.php",
            "src/CliMessage.php"
        ]
    },
    "require": {
        "wp-cli/php-cli-tools": ">=0.11.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*"
    }
}